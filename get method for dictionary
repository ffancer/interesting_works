dic = {
        'Darth Vader': 'father',
        'Leia': 'sister',
        'Han': 'brother in low',
        'R2D2': 'droid'
    }
    
def result(name):
    name = dic.get(name)
    return 'Luke, I am your ' + name

print(result("Darth Vader"))
print(result("Leia"))
print(result("Han"))
print(result("R2D2"))
